    if (int.TryParse(Console.ReadLine(), out int a) && a >= 1 && a <= 2000)
    {
        for (int i = 0; i < a; i++)
        {
            Console.ReadLine(); 
        }
        Console.WriteLine("Ciencia da Computacao");
