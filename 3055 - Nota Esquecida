// My code as a begginer .NET dev:
if (int.TryParse(Console.ReadLine(), out int note) && int.TryParse(Console.ReadLine(), out int media) && media >= 0 && note >= 0 && note <= 100 && media <= 100 )
{
    Console.WriteLine(media * 2 - note);
}